{
    "name": "trazalog/traz-comp-dnato",
    "description": "Módulo de seguridad, autorización y configuración de la suite Trazalog Tools",
    "type": "project",
    "keywords": ["trazalog", "codeigniter", "security", "authorization", "bulkload"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Trazalog Tools Team",
            "email": "dev@trazalog.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "shuchkin/simplexlsx": "^1.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Trazalog\\": "application/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "chmod 755 bulkload_stage_files"
        ],
        "post-update-cmd": [
            "chmod 755 bulkload_stage_files"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}






